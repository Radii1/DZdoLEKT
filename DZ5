#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, c;
	cout << "Введите числа: ";
	cin >> a >> b;
	if (b < a)
	{
		c = a;
		a = b;
		b = c;
	}
	cout << "Выбранный диапозон: от " << a << " до " << b << endl;
	for (int i = a;i != b + 1;i++)
	{
		c = 0;
		for (int j = 2; j < i; j++)
		{
			if (i % j == 0)
			{
				c++;
			}
		}
		if (c == 0)
		{
			cout << i << " ";
		}
	}
	return 0;
}
